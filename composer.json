{
    "name": "universityofadelaide/shepherd-drupal-scaffold",
    "description": "Composer Plugin for updating the Shepherd Drupal tools.",
    "type": "composer-plugin",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "require": {
        "php": ">=8",
        "composer-plugin-api": "^2",
        "symfony/filesystem": "~3.0 || ^4.0"
    },
    "require-dev": {
        "composer/composer": "^2@stable",
        "friendsofphp/php-cs-fixer": "^3@stable",
        "consolidation/robo": "^4"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-4": {
            "UniversityOfAdelaide\\ShepherdDrupalScaffold\\": "src/"
        }
    },
    "extra": {
        "class": "UniversityOfAdelaide\\ShepherdDrupalScaffold\\ShepherdDrupalScaffoldPlugin"
    }
}
